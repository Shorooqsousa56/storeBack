{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "node build/server.js",
    "docker:up": "docker-compose up -d && docker-compose ps",
    "docker:down": "docker-compose down",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "cross-env NODE_ENV=test dotenv -e .env.test jasmine",
    "build": "npx tsc",
    "migrate": "db-migrate up -e dev",
    "migrate-down": "db-migrate down -e dev",
    "migratetest": "db-migrate up -e test",
    "migratetest-down": "db-migrate down -e test"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "body-parser": "^1.20.3",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^4.17.24",
    "@types/jasmine": "^3.10.18",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^6.0.3",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^11.0.0",
    "jasmine": "^3.99.0",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.3",
    "supertest": "^7.1.4",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.6.2"
  }
}
